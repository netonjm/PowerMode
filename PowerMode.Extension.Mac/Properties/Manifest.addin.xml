<?xml version="1.0" encoding="UTF-8"?>
<ExtensionModel>
	<Runtime>
	</Runtime>
     <Extension path="/MonoDevelop/Ide/StartupHandlers">
        <Class class="PowerMode.InitCommandHandler"/>
    </Extension>
     <Extension path="/MonoDevelop/Ide/Composition">
        <Assembly file="PowerMode.Extension.Mac.dll"/>
    </Extension>
      <Extension path="/MonoDevelop/Ide/GlobalOptionsDialog/Projects">
        <Section id="PowerModeSettings" _label="Power Mode" class = "PowerMode.Extension.Preferences.PowerModeOptionsPanel" icon="vs-prefs-powermode" />
    </Extension>
     <Extension path = "/MonoDevelop/Core/StockIcons">
        <StockIcon stockid = "vs-prefs-powermode" resource = "vs-prefs-power.png" size = "Menu"/>
    </Extension>
</ExtensionModel>
